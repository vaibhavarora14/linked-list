{
  "name": "ecmascript-library-template",
  "version": "0.0.0-development",
  "private": false,
  "description": "Ecmascript library template to focus on ‚å®Ô∏è coding, üôå collaborating and üöÄ shipping",
  "repository": {
    "type": "git",
    "url": "https://github.com/varora1406/ecmascript-library-template.git"
  },
  "license": "MIT",
  "author": "Vaibhav Arora <varora1406@gmail.com>",
  "main": "dist/",
  "files": [
    "dist/",
    "lib/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "babel lib/ -d dist/ --source-maps --ignore '**/*.test.js' --delete-dir-on-start",
    "commit": "git cz",
    "format": "prettier --write \"**/*.*\" && eslint . --fix",
    "lint": "prettier --check \"**/*.*\" && eslint .",
    "prepublishOnly": "yarn build",
    "semantic-release": "semantic-release",
    "test": "jest --coverage && yarn lint"
  },
  "jest": {},
  "resolutions": {
    "**/lodash": "^4.17.13",
    "**/mem": "^4.0.0",
    "**/minimist": "^1.2.3",
    "**/underscore.string": "^3.3.5",
    "**/yargs-parser": "^18.1.2"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "7.10.1",
    "@babel/core": "7.10.2",
    "@babel/preset-env": "7.10.2",
    "@types/jest": "25.2.3",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.0.1",
    "eslint": "7.1.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "23.13.2",
    "git-cz": "^4.6.2",
    "jest": "26.0.1",
    "jest-date-mock": "1.0.8",
    "prettier": "2.0.5",
    "prettier-plugin-packagejson": "2.2.5",
    "semantic-release": "17.0.8",
    "semantic-release-cli": "5.3.1"
  },
  "renovate": {
    "extends": [
      "config:js-lib",
      ":automergePatch",
      ":automergeBranch",
      ":automergePatch",
      ":automergeBranch",
      ":automergeLinters",
      ":automergeTesters",
      ":automergeTypes"
    ]
  }
}
